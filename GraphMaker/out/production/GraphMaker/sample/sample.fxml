                <NumberAxis fx:id="XAxis" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="YAxis" side="LEFT" />
              </yAxis>
            </LineChart>
            <TextField fx:id="func_input" layoutX="146.0" layoutY="327.0" prefHeight="31.0" prefWidth="154.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="146.0" />
            <Label layoutX="14.0" layoutY="332.0" text="Enter function, f(x)=" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="14.0" />
