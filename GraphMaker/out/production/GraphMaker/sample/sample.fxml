            <Button fx:id="buttonSetRange" layoutX="676.0" layoutY="327.0" mnemonicParsing="false" onAction="#setRange" text="Set Range" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="540.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
